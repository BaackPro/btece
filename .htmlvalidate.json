{
  "extends": ["html-validate:recommended"],
  "rules": {
    "doctype-style": "off",
    "void-style": "off", 
    "long-title": "warn",
    "no-trailing-whitespace": "warn",
    "no-inline-style": "warn",
    "script-type": "off",
    "tel-non-breaking": "off",
    "prefer-native-element": "off"
  }
}
